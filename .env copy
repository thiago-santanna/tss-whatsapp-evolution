TRAEFIK_ENTRYPOINT_WEB=:80
TRAEFIK_ENTRYPOINT_WEBSECURE=:443

# === Domínio do n8n ===
N8N_DOMAIN=n8n.seudominio

# === Credenciais do n8n ===
N8N_BASIC_AUTH_ACTIVE=true
